ParkingLot:


public class ParkingLot{
	List<ParkingSpace> spaces;
	List<ParkingSpace> vacantSpaces;
	ParkingLotSign sign;

}

public class ParkingLotSign{
	public static void displaySpaceLeft(int spaceCount);
}

public class ParkingSpace{
	int id;
	int distance;
	boolean occupied;


}

public class Vehicle{
